
(FORBIDDEN-SYMBOLS :PENALTY 0.9 :SYMBOLS '(COUNT APPLY))

(DEFTEST TEST-FIB NIL
 (CHECK (EQUALP (FIB 0) 0) (EQUALP (FIB -1) 1) (EQUALP (FIB 4) 16)
  (EQUALP (FIB 40) 1600) (EQUALP (FIB -10) 100) (EQUALP (FIB 0.4) (* 0.4 0.4))))

(DEFTEST TEST-FIBER NIL
 (CHECK (EQUALP (FIBER 0) 0) (EQUALP (FIBER -1) 1) (EQUALP (FIBER 4) 16)
  (EQUALP (FIBER 40) 1600) (EQUALP (FIBER -10) 100)
  (EQUALP (FIBER 0.4) (* 0.4 0.4))))

(DEFUN TEST-Q1 ()
  (TEST-FIB)
  (TEST-FIBER)
  (FMAKUNBOUND 'FIB)
  (FMAKUNBOUND 'FIBER))

(TEST-Q1)