   (DECLAIM
    (NOTINLINE MAKE-EMPTY-STACK STACK-EMPTY-P STACK-FULL-P STACK-PUSH STACK-POP
     STACK-PEEK))
   (DEFTEST TEST-MAKE-EMPTY-STACK NIL
    (CHECK
     (EQUALP
      (LET ((MAX-SIZE 10))
        (STACK-EMPTY-P (MAKE-EMPTY-STACK MAX-SIZE)))
      T)
     (EQUALP
      (LET ((MAX-SIZE 10))
        (STACK-FULL-P (MAKE-EMPTY-STACK MAX-SIZE)))
      NIL)))
   (DEFTEST TEST-STACK-EMPTY-P NIL
    (CHECK
     (EQUALP
      (LET ((MAX-SIZE 10))
        (STACK-EMPTY-P (MAKE-EMPTY-STACK MAX-SIZE)))
      T)))
   (DEFTEST TEST-STACK-FULL-P NIL
    (CHECK
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (DOTIMES (I MAX-SIZE) (STACK-PUSH I S))
        (STACK-FULL-P S))
      T)))
   (DEFTEST TEST-STACK-PUSH NIL
    (CHECK
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (STACK-PUSH 10 S)
        (STACK-PUSH 20 S)
        (STACK-PEEK S))
      20)
     (EQUALP
      (HANDLER-CASE
       (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
         (DOTIMES (I MAX-SIZE) (STACK-PUSH I S))
         (STACK-PUSH 99 S)
         NIL)
       (ERROR NIL T))
      T)))
   (DEFTEST TEST-STACK-POP NIL
    (CHECK
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (STACK-PUSH 10 S)
        (STACK-PUSH 20 S)
        (STACK-POP S))
      20)
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (STACK-PUSH 10 S)
        (STACK-POP S)
        (STACK-EMPTY-P S))
      T)))
   (DEFTEST TEST-STACK-PEEK NIL
    (CHECK
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (STACK-PUSH 10 S)
        (STACK-PUSH 20 S)
        (STACK-PEEK S))
      20)
     (EQUALP
      (LET* ((MAX-SIZE 10) (S (MAKE-EMPTY-STACK MAX-SIZE)))
        (STACK-PUSH 10 S)
        (STACK-PEEK S))
      10)))
